[sub_resource type="GDScript" id="GDScript_aou51"]
script/source = "extends VBoxContainer
@onready var spiel_modus_index_1: VBoxContainer = $\"Spiel-Modus-index-1\"
@onready var spiel_modus_index_0: VBoxContainer = $Spiel_Modus_index_0

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	get_tree_string_pretty()
	pass # Replace with function body.


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	pass






func _on_rule_mode_id_pressed(id: int) -> void:
	ConwaysGameSlow.Logging((\"Modus auswahl hat die id \"), id)
	
	if spiel_modus_index_1 == null:
		ConwaysGameSlow.Logging(\"Modus index 1 ist null\", true)
		return
	if id == 0:
		spiel_modus_index_0.show()
	else:
		spiel_modus_index_0.hide()
	if id == 1:
		spiel_modus_index_1.show()
	else:
		spiel_modus_index_1.hide()

		
"

